{
  "name": "Tomothgotchi",
  "version": "1.0",
  "manifest_version": 2,
    "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "icons": { 
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_title": "Tomothgotchi",
    "default_popup":"popup.html"
}
}
